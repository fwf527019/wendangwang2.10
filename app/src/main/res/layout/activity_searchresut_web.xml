<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/sub_nonetwork"></include>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <hb.xnwdw.com.wendangwang.gui.view.ScrollerWebview
            android:id="@+id/search_web"
            android:layout_width="match_parent"
            android:layout_height="match_parent"></hb.xnwdw.com.wendangwang.gui.view.ScrollerWebview>

        <ImageView
            android:id="@+id/resutweb_back"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:padding="10dp" />
    </FrameLayout>
</LinearLayout>