<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/url_web_tital"
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:background="@color/white">

        <ImageView
            android:id="@+id/urlactivty_back"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_centerVertical="true"
            android:layout_margin="5dp"
            android:src="@drawable/back_icon" />

        <TextView
            android:id="@+id/urlactivty_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:textColor="@color/black"
            android:textSize="@dimen/px34" />

        <ImageView
            android:id="@+id/urlactivty_shear"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/share_btn" />
    </RelativeLayout>
    <include layout="@layout/sub_nonetwork"></include>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <hb.xnwdw.com.wendangwang.gui.view.ScrollerWebview
            android:id="@+id/urlwebview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"></hb.xnwdw.com.wendangwang.gui.view.ScrollerWebview>
        <ImageView
            android:id="@+id/urlweb_back"
            android:layout_width="45dp"
         android:visibility="gone"
            android:layout_height="45dp" />
    </FrameLayout>
</LinearLayout>